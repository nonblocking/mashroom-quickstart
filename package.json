{
    "name": "@mashroom/mashroom-quickstart",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=18"
    },
    "workspaces": [
        "./plugin-packages/*"
    ],
    "dependencies": {
        "@mashroom/mashroom": "^2.8.1",
        "@mashroom/mashroom-docs-static": "^2.8.1",
        "@mashroom/mashroom-browser-cache": "^2.8.1",
        "@mashroom/mashroom-helmet": "^2.8.1",
        "@mashroom/mashroom-i18n": "^2.8.1",
        "@mashroom/mashroom-error-pages": "^2.8.1",
        "@mashroom/mashroom-security": "^2.8.1",
        "@mashroom/mashroom-security-default-login-webapp": "^2.8.1",
        "@mashroom/mashroom-security-provider-simple": "^2.8.1",
        "@mashroom/mashroom-session": "^2.8.1",
        "@mashroom/mashroom-session-provider-filestore": "^2.8.1",
        "@mashroom/mashroom-storage": "^2.8.1",
        "@mashroom/mashroom-storage-provider-filestore": "^2.8.1",
        "@mashroom/mashroom-json-schemas": "^2.8.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.27.0",
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-typescript": "^7.27.0",
        "eslint": "^9.23.0",
        "@eslint/js": "^9.23.0",
        "@eslint/json": "^0.11.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.11.0",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "@typescript-eslint/parser": "8.29.0",
        "@typescript-eslint/eslint-plugin": "8.29.0",
        "typescript": "^5.8.2",
        "@types/jest": "^29.5.14",
        "@types/express": "^4.17.21"
    },
    "scripts": {
        "type-check": "npm run type-check --workspaces",
        "lint:config": "eslint -c ./config/eslint.config.mjs ./config",
        "lint": "npm run lint --workspaces && npm run lint:config",
        "build": "npm run type-check && npm run lint && npm run build --workspaces",
        "start": "node ./mashroom-starter",
        "docker:create-image": "docker build -t mashroom-quickstart .",
        "docker:start": "docker run -it -e NODE_ENV=production -p 6060:6060 mashroom-quickstart "
    }
}
