{
    "name": "@mashroom/mashroom-quickstart",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=16"
    },
    "workspaces": [
        "./plugin-packages/*"
    ],
    "dependencies": {
        "@mashroom/mashroom": "^2.4.4",
        "@mashroom/mashroom-docs-static": "^2.4.4",
        "@mashroom/mashroom-browser-cache": "^2.4.4",
        "@mashroom/mashroom-helmet": "^2.4.4",
        "@mashroom/mashroom-i18n": "^2.4.4",
        "@mashroom/mashroom-error-pages": "^2.4.4",
        "@mashroom/mashroom-security": "^2.4.4",
        "@mashroom/mashroom-security-default-login-webapp": "^2.4.4",
        "@mashroom/mashroom-security-provider-simple": "^2.4.4",
        "@mashroom/mashroom-session": "^2.4.4",
        "@mashroom/mashroom-session-provider-filestore": "^2.4.4",
        "@mashroom/mashroom-storage": "^2.4.4",
        "@mashroom/mashroom-storage-provider-filestore": "^2.4.4",
        "@mashroom/mashroom-json-schemas": "^2.4.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.15",
        "@babel/core": "^7.22.15",
        "@babel/preset-env": "^7.22.15",
        "@babel/preset-typescript": "^7.22.15",
        "eslint": "^8.48.0",
        "eslint-plugin-jest": "^27.2.3",
        "eslint-plugin-json": "^3.1.0",
        "jest": "^29.6.4",
        "@typescript-eslint/parser": "6.6.0",
        "@typescript-eslint/eslint-plugin": "6.6.0",
        "typescript": "^5.2.2",
        "@types/jest": "^29.5.4",
        "@types/express": "^4.17.17"
    },
    "scripts": {
        "type-check": "npm run type-check --workspaces",
        "lint:config": "eslint --ext .js,.json --fix ./config",
        "lint": "npm run lint --workspaces && npm run lint:config",
        "build": "npm run type-check && npm run lint && npm run build --workspaces",
        "start": "node ./mashroom-starter",
        "docker:create-image": "docker build -t mashroom-quickstart .",
        "docker:start": "docker run -it -e NODE_ENV=production -p 6060:6060 mashroom-quickstart "
    }
}
