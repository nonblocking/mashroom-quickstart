{
    "name": "@mashroom/mashroom-quickstart",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=14"
    },
    "dependencies": {
        "@mashroom/mashroom": "^2.1.2",
        "@mashroom/mashroom-docs-static": "^2.1.2",
        "@mashroom/mashroom-browser-cache": "^2.1.2",
        "@mashroom/mashroom-csrf-protection": "^2.1.2",
        "@mashroom/mashroom-helmet": "^2.1.2",
        "@mashroom/mashroom-i18n": "^2.1.2",
        "@mashroom/mashroom-error-pages": "^2.1.2",
        "@mashroom/mashroom-security": "^2.1.2",
        "@mashroom/mashroom-security-default-login-webapp": "^2.1.2",
        "@mashroom/mashroom-security-provider-simple": "^2.1.2",
        "@mashroom/mashroom-session": "^2.1.2",
        "@mashroom/mashroom-session-provider-filestore": "^2.1.2",
        "@mashroom/mashroom-storage": "^2.1.2",
        "@mashroom/mashroom-storage-provider-filestore": "^2.1.2",
        "@mashroom/mashroom-json-schemas": "^2.1.2",
        "lerna": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-typescript": "^7.17.12",
        "eslint": "^8.17.0",
        "eslint-plugin-jest": "^26.5.3",
        "eslint-plugin-json": "^3.1.0",
        "@typescript-eslint/parser": "5.27.1",
        "@typescript-eslint/eslint-plugin": "5.27.1",
        "typescript": "^4.7.3",
        "@types/jest": "^28.1.1",
        "@types/express": "^4.17.13"
    },
    "scripts": {
        "setup": "npm install && lerna bootstrap --no-ci",
        "type-check": "lerna run type-check",
        "lint:config": "eslint --ext .js,.json --fix ./config",
        "lint": "lerna run lint && npm run lint:config",
        "build": "npm run type-check && npm run lint && lerna run build",
        "start": "node ./mashroom-starter",
        "docker:create-image": "docker build -t mashroom-quickstart .",
        "docker:start": "docker run -e NODE_ENV=production -p 6060:6060 mashroom-quickstart "
    }
