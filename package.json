{
    "name": "@mashroom/mashroom-quickstart",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@mashroom/mashroom": "^2.4.0",
        "@mashroom/mashroom-docs-static": "^2.4.0",
        "@mashroom/mashroom-browser-cache": "^2.4.0",
        "@mashroom/mashroom-helmet": "^2.4.0",
        "@mashroom/mashroom-i18n": "^2.4.0",
        "@mashroom/mashroom-error-pages": "^2.4.0",
        "@mashroom/mashroom-security": "^2.4.0",
        "@mashroom/mashroom-security-default-login-webapp": "^2.4.0",
        "@mashroom/mashroom-security-provider-simple": "^2.4.0",
        "@mashroom/mashroom-session": "^2.4.0",
        "@mashroom/mashroom-session-provider-filestore": "^2.4.0",
        "@mashroom/mashroom-storage": "^2.4.0",
        "@mashroom/mashroom-storage-provider-filestore": "^2.4.0",
        "@mashroom/mashroom-json-schemas": "^2.4.0",
        "lerna": "^6.6.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.5",
        "@babel/core": "^7.22.5",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "eslint": "^8.42.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-json": "^3.1.0",
        "jest": "^29.5.0",
        "@typescript-eslint/parser": "5.59.9",
        "@typescript-eslint/eslint-plugin": "5.59.9",
        "typescript": "^5.1.3",
        "@types/jest": "^29.5.2",
        "@types/express": "^4.17.17"
    },
    "scripts": {
        "setup": "npm install && lerna bootstrap --no-ci",
        "type-check": "lerna run type-check",
        "lint:config": "eslint --ext .js,.json --fix ./config",
        "lint": "lerna run lint && npm run lint:config",
        "build": "npm run type-check && npm run lint && lerna run build",
        "start": "node ./mashroom-starter",
        "docker:create-image": "docker build -t mashroom-quickstart .",
        "docker:start": "docker run -e NODE_ENV=production -p 6060:6060 mashroom-quickstart "
