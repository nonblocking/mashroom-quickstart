{
    "name": "@mashroom/mashroom-quickstart",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "@mashroom/mashroom": "^1.0.90",
        "@mashroom/mashroom-browser-cache": "^1.0.90",
        "@mashroom/mashroom-csrf-protection": "^1.0.90",
        "@mashroom/mashroom-docs-static": "^1.0.90",
        "@mashroom/mashroom-i18n": "^1.0.90",
        "@mashroom/mashroom-security": "^1.0.90",
        "@mashroom/mashroom-security-default-login-webapp": "^1.0.90",
        "@mashroom/mashroom-security-provider-simple": "^1.0.90",
        "@mashroom/mashroom-session": "^1.0.90",
        "@mashroom/mashroom-session-provider-filestore": "^1.0.90",
        "@mashroom/mashroom-storage": "^1.0.90",
        "@mashroom/mashroom-storage-provider-filestore": "^1.0.90",
        "lerna": "3.15.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-flow": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-flowtype": "^3.10.3",
        "eslint-plugin-jest": "^22.6.4",
        "flow-bin": "^0.101.0"
    },
    "scripts": {
        "setup": "npm install && lerna bootstrap --no-ci",
        "setup-production": "npm install --production && NODE_ENV=production lerna bootstrap --no-ci",
        "flow": "flow check",
        "lint": "lerna run lint",
        "build": "npm run flow && npm run lint && lerna run build",
        "start": "node ./mashroom-starter",
        "docker:create-image": "npm run setup && npm run build && docker build -t mashroom-quickstart .",
        "docker:start": "docker run -e NODE_ENV=production -p 6060:6060 mashroom-quickstart "
    }
}
