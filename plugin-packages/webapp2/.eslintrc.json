{
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint/eslint-plugin"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {
        "node": true,
        "browser": false,
        "es6": true,
        "jest": true
    },
    "rules": {
        "no-unused-vars": "off",
        "quotes": ["error", "single", { "allowTemplateLiterals": true }]
    }
}
